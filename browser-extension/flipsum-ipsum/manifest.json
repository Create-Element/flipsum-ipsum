{
   "manifest_version": 2,
   "name": "Flipsum Ipsum",
   "version": "1.1.1",
   "description": "Access loads of gobbledygook from your browser.",
   "homepage_url": "https://power-plugins.com/",
   "icons": {
      "32": "icons/flipsum-ipsum-32.png",
      "48": "icons/flipsum-ipsum-48.png",
      "96": "icons/flipsum-ipsum-96.png"
   },
   "permissions": [
      "https://power-plugins.com/*",
      "clipboardWrite",
      "storage"
   ],
   "browser_action": {
      "default_icon": "icons/flipsum-ipsum-32.png",
      "default_title": "Flipsum Ipsum",
      "default_popup": "popup/generate_ipsum.html"
   },
   "web_accessible_resources": [
      "assets/*.svg"
   ]
}